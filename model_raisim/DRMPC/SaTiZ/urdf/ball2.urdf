<?xml version="1.0" ?>
<robot name="LISM_Arm">
     <!--base of fixing arm-->
    <link name="world"/>

    <joint name="worldTolink1" type="fixed">
        <parent link="world"/>
        <child link="link0"/>
        <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
    </joint>

    <link name = "link0"></link>
    
    <joint name="y" type="prismatic">
        <parent link="link0"/>
        <child link="link1"/>
        <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
        <axis xyz="0.0 1.0 0.0"/>
    </joint>

    <link name = "link1"></link>

    <joint name="z" type="prismatic">
        <parent link="link1"/>
        <child link="ball"/>
        <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
        <axis xyz="0.0 0.0 1"/>
    </joint>

    <link name="ball">
        <visual>
            <origin rpy="0.0 0.0 0" xyz="0 0 0.0"/>
            <geometry>
                <!-- <sphere radius="0.2"/> -->
                <mesh filename="meshes/ball2.dae" scale="0.001 0.001 0.001"/>
                <!-- <mesh filename="meshes/ball/ball2.OBJ" scale="100 100 100"/> -->
            </geometry>
        </visual>
        <inertial>
            <mass value="0.4"/>
            <origin rpy="0.0 0.0 0.0" xyz="0 0 0"/>
            <inertia ixx="0.00267" ixy="0" ixz="0.0" iyy="0.00267" iyz="0" izz="0.00267"/>
        </inertial>
        <collision>
            <origin rpy="0.0 0.0 0.0" xyz="0 0.0 0.0"/>
            <geometry>
                <sphere radius="0.15"/>                  
            </geometry>
            <material name="">
                <contact name="rubber"/>
            </material>
        </collision>
    </link>

    <!-- <joint name="worldToball" type="fixed">
        <parent link="world"/>
        <child link="ball"/>
        <origin rpy="0 0 0" xyz="0 0 0"/>
    </joint> -->

</robot>