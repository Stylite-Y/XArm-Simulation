<?xml version="1.0" ?>
<robot name="LISM_Arm">

    <!-- Basic colors -->
    <material name="black">
        <color rgba="0.0 0.0 0.0 1.0"/>
    </material>
    <material name="greyblack">
        <color rgba="0.8 0.8 0.8 1"/>
    </material>
    <material name="blue">
        <color rgba="0.0 0.0 0.8 1.0"/>
    </material>
    <material name="green">
        <color rgba="0.0 0.8 0.0 1.0"/>
    </material>
    <material name="grey">
        <color rgba="0.3 0.3 0.3 1"/>
    </material>
    <material name="silver">
        <color rgba="0.8 0.8 0.8 1"/>
    </material>
    <material name="red">
        <color rgba="0.8 0.0 0.0 1.0"/>
    </material>
    <material name="white">
        <color rgba="1.0 1.0 1.0 1.0"/>
    </material>

    <material name="anymal_material">
        <color rgba="0.7 0.7 0.7 1.0"/>
    </material>


    <!-- Material for the visual primitives -->
    <material name="anymal_material">
        <color rgba="0.7 0.7 0.7 1.0"/>
    </material>

<!---=============================the base of fixing arm======================-->
    <!--base of fixing arm-->
    <link name="world"/>

    <joint name="worldTolink1" type="fixed">
        <parent link="world"/>
        <child link="base_plate"/>
        <origin xyz="0 0 0"/>
    </joint>

    <link name="base_plate">
        <visual>
            <origin rpy="0.0 0.0 0.0" xyz="0 0 0"/>
            <geometry>
                <box size="0.2 0.3 0.02"/>
            </geometry>
            <material name="anymal_material"/>
        </visual>
        
    </link>

    <joint name="base_fix" type="fixed">
        <axis xyz="1 0 0"/>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <parent link="base_plate"/>
        <child link="fix_pillar"/>
    </joint>

    <link name="fix_pillar">
        <visual>
            <origin rpy="0.0 0.0 0.0" xyz="0 0 0.4"/>
            <geometry>
                <cylinder length="0.8" radius="0.05"/>
            </geometry>
            <material name="greyblack"/>
        </visual>
    </link>

    <joint name="fix_plate" type="fixed">
        <axis xyz="1 0 0"/>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <parent link="fix_pillar"/>
        <child link="arm_plate"/>
    </joint>

    <link name="arm_plate">
        <visual>
            <origin rpy="0.0 0.0 0.0" xyz="0 0 0.7"/>
            <geometry>
                <box size="0.1 0.2 0.1"/>
            </geometry>
            <material name="greyblack"/>
        </visual>
    </link>

    <joint name="plate_cir_r" type="fixed">
        <axis xyz="1 0 0"/>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <parent link="arm_plate"/>
        <child link="cir_plate_r"/>
    </joint>

    <link name="cir_plate_r">
        <visual>
            <origin rpy="0.0 1.57 0.0" xyz="0 0.1 0.7"/>
            <geometry>
                <cylinder length="0.1" radius="0.05"/>
            </geometry>
            <material name="greyblack"/>
        </visual>
    </link>

    <joint name="plate_cir_l" type="fixed">
        <axis xyz="1 0 0"/>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <parent link="arm_plate"/>
        <child link="cir_plate_l"/>
    </joint>

    <link name="cir_plate_l">
        <visual>
            <origin rpy="0.0 1.57 0.0" xyz="0 -0.1 0.7"/>
            <geometry>
                <cylinder length="0.1" radius="0.05"/>
            </geometry>
            <material name="silver"/>
        </visual>
    </link>

    <joint name="plate_arm_r" type="revolute">
        <axis xyz="0 0 1"/>
        <origin rpy="0 0 0" xyz="0.05 0.1 0.7"/>
        <parent link="cir_plate_r"/>
        <child link="upper_r"/>
        <limit effort="180" lower="0.0" upper="3.14" velocity="400"/>
        <!-- <dynamics damping="0.01" rotor_inertia="0.003708"/> -->
    </joint>

    <!-- <joint name="plate_arm_l" type="continuous">
        <axis xyz="1 0 0"/>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <parent link="cir_plate_l"/>
        <child link="abad_l"/>
    </joint> -->

<!---============================= Right Arm ======================-->
       <!--upper link-->
    <link name="upper_r">
        <visual>
            <origin rpy="0 1.57 0" xyz="0.15 0.0 0"/>
            <geometry>
                <cylinder length="0.3" radius="0.02"/>
            </geometry>
            <material name="greyblack"/>
        </visual>
        <inertial>
            <mass value="1.8"/>
            <origin rpy="0.0 0.0 0.0" xyz="0.15 0.0 0.0"/>
            <inertia ixx="0.00036" ixy="0.0" ixz="0.0"
                     iyy="0.0135" iyz="0.0" izz="0.0135"/>
        </inertial>
         <!-- <collision>
            <origin rpy="1.57 0.0 0.0" xyz="0.0 0.0 0"/>
            <geometry>
                <cylinder length="0.035" radius="0.05"/>
            </geometry>
        </collision>
        <collision>
            <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 -0.12"/>
            <geometry>
                <box size="0.05 0.04 0.15"/>
            </geometry>
        </collision> -->
    </link>

    <!--upper to lower joint-->
    <joint name="upper_lower_r" type="revolute">
        <axis xyz="0 0 1"/>
        <origin rpy="0 0 0" xyz="0.3 0.0 0.0"/>
        <parent link="upper_r"/>
        <child link="lower_r"/>
        <limit effort="180" lower="-3.14" upper="0" velocity="400"/>
        <!-- <dynamics damping="0.01" rotor_inertia="0.008966"/>
         -->
    </joint>

    <!--lower link-->
    <link name="lower_r">
        <visual>
            <!-- <origin rpy="0.0 0.0 3.14" xyz="-0.025 0.032 0"/> -->
            <origin rpy="0 1.57 0" xyz="0.15 0.0 0"/>
            <geometry>
                <cylinder length="0.3" radius="0.02"/>
            </geometry>
            <material name="greyblack"/>
        </visual>
        <inertial>
            <mass value="1.0"/>
            <origin rpy="0.0 0.0 0.0" xyz="0.15 0.0 0.0"/>
            <inertia ixx="0.00036" ixy="0.0" ixz="0.0"
                     iyy="0.0135" iyz="0.0" izz="0.0135"/>
        </inertial>
        <!-- <collision>
            <origin rpy="0.0 0.0 0.0" xyz="0 0.0 -0.1"/>
            <geometry>
                <box size="0.04 0.03 0.18"/>
            </geometry>
        </collision> -->
    </link>

    <joint name="toe_fr_joint" type="fixed">
        <axis xyz="1 0 0"/>
        <parent link="lower_r"/>
        <child link="toe_fr"/>
        <origin xyz="0.3 0.0 0"/>
        <dynamics damping="0.0" friction="0.0"/>
    </joint>

    <link name="toe_fr">
        <contact>
            <friction_anchor/>
            <stiffness value="30000.0"/>
            <damping value="1000.0"/>
            <spinning_friction value="0.3"/>
            <lateral_friction value="3.0"/>
        </contact>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <sphere radius="0.03"/>
            </geometry>
            <material name="">
                <contact name="rub"/>
            </material>
        </collision>
        <inertial>
            <mass value="0.01"/>
            <inertia ixx="0.0000036" ixy="0" ixz="0" iyy="0.0000036" iyz="0" izz="0.0000036"/>
        </inertial>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <sphere radius="0.03"/>
            </geometry>
            <material name="greyblack"/>
        </visual>
    </link>

</robot>