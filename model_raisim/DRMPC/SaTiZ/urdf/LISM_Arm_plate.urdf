<?xml version="1.0" ?>
<robot name="LISM_Arm">

    <!-- Basic colors -->
    <material name="black">
        <color rgba="0.0 0.0 0.0 1.0"/>
    </material>
    <material name="greyblack">
        <color rgba="0.1765 0.1765 0.1765 1"/>
    </material>
    <material name="blue">
        <color rgba="0.0 0.0 0.8 1.0"/>
    </material>
    <material name="green">
        <color rgba="0.0 0.8 0.0 1.0"/>
    </material>
    <material name="grey">
        <color rgba="0.3 0.3 0.3 1"/>
    </material>
    <material name="silver">
        <color rgba="0.8 0.8 0.8 1"/>
    </material>
    <material name="red">
        <color rgba="0.8 0.0 0.0 1.0"/>
    </material>
    <material name="white">
        <color rgba="1.0 1.0 1.0 1.0"/>
    </material>

    <material name="anymal_material">
        <color rgba="0.7 0.7 0.7 1.0"/>
    </material>


    <!-- Material for the visual primitives -->
    <material name="anymal_material">
        <color rgba="0.7 0.7 0.7 1.0"/>
    </material>

<!---=============================the base of fixing arm======================-->
    <!--base of fixing arm-->
    <link name="world"/>

    <joint name="worldTolink1" type="fixed">
        <parent link="world"/>
        <child link="base_plate"/>
        <origin xyz="0 0 0"/>
    </joint>

    <link name="base_plate">
        <visual>
            <origin rpy="0.0 0.0 0.0" xyz="0 0 0"/>
            <geometry>
                <box size="0.2 0.3 0.02"/>
            </geometry>
            <material name="anymal_material"/>
        </visual>
        
    </link>

    <joint name="base_fix" type="fixed">
        <axis xyz="1 0 0"/>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <parent link="base_plate"/>
        <child link="fix_pillar"/>
    </joint>

    <link name="fix_pillar">
        <visual>
            <origin rpy="0.0 0.0 0.0" xyz="0 0 0.4"/>
            <geometry>
                <cylinder length="0.8" radius="0.05"/>
            </geometry>
            <material name="black"/>
        </visual>
    </link>

    <joint name="fix_plate" type="fixed">
        <axis xyz="0 0 1"/>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <parent link="fix_pillar"/>
        <child link="arm_plate"/>
    </joint>

    <link name="arm_plate">
        <visual>
            <origin rpy="0.0 0.0 0.0" xyz="0 0 0.7"/>
            <geometry>
                <box size="0.1 0.2 0.1"/>
            </geometry>
            <material name="greyblack"/>
        </visual>
    </link>

    <joint name="plate_cir_r" type="fixed">
        <axis xyz="1 0 0"/>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <parent link="arm_plate"/>
        <child link="cir_plate_r"/>
    </joint>

    <link name="cir_plate_r">
        <visual>
            <origin rpy="0.0 1.57 0.0" xyz="0 0.1 0.7"/>
            <geometry>
                <cylinder length="0.1" radius="0.05"/>
            </geometry>
            <material name="greyblack"/>
        </visual>
    </link>

    <joint name="plate_cir_l" type="fixed">
        <axis xyz="1 0 0"/>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <parent link="arm_plate"/>
        <child link="cir_plate_l"/>
    </joint>

    <link name="cir_plate_l">
        <visual>
            <origin rpy="0.0 1.57 0.0" xyz="0 -0.1 0.7"/>
            <geometry>
                <cylinder length="0.1" radius="0.05"/>
            </geometry>
            <material name="silver"/>
        </visual>
    </link>

    <!-- <joint name="plate_arm_r" type="revolute">
        <axis xyz="1 0 0"/>
        <origin rpy="0 0 0" xyz="0.05 0.1 0.7"/>
        <parent link="cir_plate_r"/>
        <child link="abad_r"/>
        <limit effort="18" lower="-6.28" upper="6.28" velocity="40"/>
        <dynamics damping="0.01" rotor_inertia="0.003708"/>
    </joint> -->
    
    <joint name="plate_arm_r" type="revolute">
        <axis xyz="1 0 0"/>
        <origin rpy="0 0 0" xyz="0.05 0.1 0.7"/>
        <parent link="arm_plate"/>
        <child link="abad_r"/>
        <dynamics damping="0.01" rotor_inertia="0.003708"/>
    </joint>

    <!-- <joint name="plate_arm_l" type="continuous">
        <axis xyz="1 0 0"/>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <parent link="cir_plate_l"/>
        <child link="abad_l"/>
    </joint> -->

<!---============================= Right Arm ======================-->
    <!--abad link-->
    <link name="abad_r">
        <visual>
            <origin rpy="0 0.0 0.0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="meshes/bp3_dae/BP3_ABAD_L.dae"/>
            </geometry>
            <material name="greyblack"/>
        </visual>
        <inertial>
            <!-- <mass value="0.54"/> -->
            <mass value="0.54"/>
            <origin rpy="0.0 0.0 0.0" xyz="0.075 0 0"/>
            <inertia ixx="0.000391" ixy="0.000000" ixz="0.000000"
                     iyy="0.000739" iyz="0.000000" izz="0.000488"/>
        </inertial> 
        <!-- <collision>
            <origin rpy="0.0 1.57 0.0" xyz="0.0 0.0 0"/>
            <geometry>
                <cylinder length="0.14" radius="0.06"/>
            </geometry>
        </collision> -->
    </link>

    <!--abad to upper joint-->
    <joint name="abad_upper_r" type="revolute">
        <axis xyz="0 1 0"/>
        <origin rpy="0 0 0" xyz="0.09 0.08 0.0"/>
        <parent link="abad_r"/>
        <child link="upper_r"/>
        <!-- <limit effort="18" lower="-6.28" upper="6.28" velocity="40"/> -->
        <!-- <dynamics damping="0.01" rotor_inertia="0.003708"/> -->
    </joint>

    <!--upper link-->
    <link name="upper_r">
        <visual>
            <origin rpy="0 0.0 0" xyz="0.0 0.0 0"/>
            <geometry>
                <mesh filename="meshes/bp3_dae/BP3_UPPER_L.dae"/>
                <!-- <mesh filename="../meshes/human_dae/lower.dae"/> -->
            </geometry>
            <material name="greyblack"/>
        </visual>
        <inertial>
            <!-- <mass value="0.036"/> -->
            <mass value="1.28"/>
            <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0"/>
            <inertia ixx="0.0036" ixy="0.0" ixz="0.0"
                     iyy="0.004" iyz="0.0004" izz="0.001"/>
        </inertial>
         <!-- <collision>
            <origin rpy="1.57 0.0 0.0" xyz="0.0 0.0 0"/>
            <geometry>
                <cylinder length="0.035" radius="0.05"/>
            </geometry>
        </collision>
        <collision>
            <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 -0.12"/>
            <geometry>
                <box size="0.05 0.04 0.15"/>
            </geometry>
        </collision> -->
    </link>

    <!--upper to lower joint-->
    <joint name="upper_lower_r" type="revolute">
        <axis xyz="0 1 0"/>
        <origin rpy="0 0 0" xyz="0.0 0.0 -0.2"/>
        <parent link="upper_r"/>
        <child link="lower_r"/>
        <!-- <dynamics damping="0.01" rotor_inertia="0.008966"/>
         -->
    </joint>

    <!--lower link-->
    <link name="lower_r">
        <visual>
            <!-- <origin rpy="0.0 0.0 3.14" xyz="-0.025 0.032 0"/> -->
            <origin rpy="0.0 -1.5708 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="meshes/bp3_dae/BP3_LOWER.dae"/>
            </geometry>
            <material name="greyblack"/>
        </visual>
        <inertial>
            <mass value="0.128"/>
            <origin rpy="0.0 0.0 0.0" xyz="0 0.032 0"/>
            <inertia ixx="0.0014" ixy="0" ixz="0.0" iyy="0.0014" iyz="0" izz="0.000024"/>
        </inertial>
        <!-- <collision>
            <origin rpy="0.0 0.0 0.0" xyz="0 0.0 -0.1"/>
            <geometry>
                <box size="0.04 0.03 0.18"/>
            </geometry>
        </collision> -->
    </link>

    <joint name="lower_add" type="revolute">
        <axis xyz="0 1 0"/>
        <origin rpy="0 0 0" xyz="0.2 0.0 0.0"/>
        <parent link="lower_r"/>
        <child link="anothor"/>
    </joint>

    <!--lower link-->
    <link name="anothor">
    </link>    

    <joint name="lower_hand_y" type="revolute">
        <axis xyz="1 0 0"/>
        <origin rpy="0 0 0" xyz="0.0, 0.0, 0.0"/>
        <parent link="anothor"/>
        <child link="hand"/>
        <!-- <dynamics damping="0.01" rotor_inertia="0.008966"/>
         -->
    </joint>

    <!--lower link-->
    <link name="hand">
        <visual>
            <!-- <origin rpy="0.0 0.0 3.14" xyz="-0.025 0.032 0"/> -->
            <origin rpy="0.0 0.0 0" xyz="0.06 0 0.0"/>
            <geometry>
                <box size="0.12 0.12 0.003"/>
            </geometry>
            <material name="greyblack"/>
        </visual>
        <inertial>
            <mass value="0.128"/>
            <origin rpy="0.0 0.0 0.0" xyz="0 0.0 0"/>
            <inertia ixx="0.00140" ixy="0" ixz="0.0" iyy="0.00132" iyz="0" izz="0.00028"/>
        </inertial>
        <collision>
            <origin rpy="0.0 0.0 0.0" xyz="0.06 0.0 0.0"/>
            <geometry>
                <box size="0.12 0.12 0.003"/>
            </geometry>
        </collision>
    </link>

</robot>