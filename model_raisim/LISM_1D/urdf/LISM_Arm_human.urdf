<?xml version="1.0" ?>
<robot name="LISM_Arm">

    <!-- Basic colors -->
    <material name="black">
        <color rgba="0.0 0.0 0.0 1.0"/>
    </material>
    <material name="greyblack">
        <color rgba="0.1765 0.1765 0.1765 1"/>
    </material>
    <material name="blue">
        <color rgba="0.0 0.0 0.8 1.0"/>
    </material>
    <material name="green">
        <color rgba="0.0 0.8 0.0 1.0"/>
    </material>
    <material name="grey">
        <color rgba="0.3 0.3 0.3 1"/>
    </material>
    <material name="silver">
        <color rgba="0.8 0.8 0.8 1"/>
    </material>
    <material name="red">
        <color rgba="0.8 0.0 0.0 1.0"/>
    </material>
    <material name="white">
        <color rgba="1.0 1.0 1.0 1.0"/>
    </material>

    <material name="anymal_material">
        <color rgba="0.7 0.7 0.7 1.0"/>
    </material>


    <!-- Material for the visual primitives -->
    <material name="anymal_material">
        <color rgba="0.7 0.7 0.7 1.0"/>
    </material>

<!---=============================the base of fixing arm======================-->
    <!--base of fixing arm-->
    <link name="base_plate">
        <visual>
            <origin rpy="0.0 0.0 0.0" xyz="0 0 0"/>
            <geometry>
                <box size="0.2 0.3 0.02"/>
            </geometry>
            <material name="anymal_material"/>
        </visual>
        
    </link>

    <joint name="base_fix" type="fixed">
        <axis xyz="1 0 0"/>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <parent link="base_plate"/>
        <child link="fix_pillar"/>
    </joint>

    <link name="fix_pillar">
        <visual>
            <origin rpy="0.0 0.0 0.0" xyz="0 0 0.25"/>
            <geometry>
                <cylinder length="0.5" radius="0.05"/>
            </geometry>
            <material name="black"/>
        </visual>
    </link>

    <joint name="fix_plate" type="fixed">
        <axis xyz="1 0 0"/>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <parent link="fix_pillar"/>
        <child link="body"/>
    </joint>

    <link name="body">
        <visual>
            <origin rpy="0.0 0.0 0.0" xyz="0 0 0.25"/>
            <geometry>
                <mesh filename="./meshes/human_dae/body.dae"/>
            </geometry>
            <material name="greyblack"/>
        </visual>
    </link>

    <joint name="body_arm_r" type="fixed">
        <axis xyz="1 0 0"/>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <parent link="body"/>
        <child link="abad_r"/>
    </joint>

    <joint name="body_arm_l" type="fixed">
        <axis xyz="1 0 0"/>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <parent link="body"/>
        <child link="abad_l"/>
    </joint>

<!---============================= Right Arm ======================-->
    <!--abad link-->
    <link name="abad_r">
        <visual>
            <origin rpy="0.0 0.0 0.0" xyz="-0.12 0.14 1.12"/>
            <geometry>
                <!-- <mesh filename="../meshes/dae/bp3_dae/BP3_ABAD_R.dae"/> -->
                <mesh filename="../meshes/human_dae/upper_r.dae"/>
            </geometry>
            <material name="greyblack"/>
        </visual>
        <inertial>
            <mass value="0.45"/>
            <origin rpy="0.0 0.0 0.0" xyz="0.0 0.3 0.0"/>
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
        </inertial> 0
        <collision>
            <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0"/>
            <geometry>
                <cylinder length="0.0215" radius="0.048"/>
            </geometry>
        </collision>
    </link>

    <!--abad to upper joint-->
    <joint name="abad_upper_r" type="continuous">
        <axis xyz="1 0 0"/>
        <origin rpy="0 0 0" xyz="0.05 0.1 0.9"/>
        <parent link="abad_r"/>
        <child link="upper_r"/>
        <limit effort="200" lower="-3.14" upper="3.14" velocity="20"/>
        <dynamics damping="0.01" friction="0.2"/>
    </joint>

    <!--upper link-->
    <link name="upper_r">
        <visual>
            <origin rpy="0 0.0 0" xyz="0.75 -0.1 -0.65"/>
            <geometry>
                <!-- <mesh filename="../meshes/dae/bp3_dae/BP3_UPPER_L.dae"/> -->
                <mesh filename="../meshes/human_dae/lower_r.dae"/>
            </geometry>
            <material name="greyblack"/>
        </visual>
        <inertial>
            <mass value="0.5"/>
            <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0"/>
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
        </inertial>
        <collision>
            <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0"/>
            <geometry>
                <cylinder length="0.023" radius="0.048"/>
            </geometry>
        </collision>
    </link>

    <!--upper to lower joint-->
    <!-- <joint name="upper_lower_r" type="continuous">
        <axis xyz="0 1 0"/>
        <origin rpy="0 0 0" xyz="0.05 0.05 -0.22"/>
        <parent link="upper_r"/>
        <child link="lower_r"/>
        <limit effort="200" lower="-3.14" upper="3.14" velocity="20"/>
        <dynamics damping="0.01" friction="0.2"/>
    </joint> -->

    <!--lower link-->
    <link name="lower_r">
        <visual>
            <origin rpy="0.0 0.0 3.14" xyz="-0.5 0.032 0"/>
            <geometry>
                <!-- <mesh filename="../meshes/dae/bp_lower.dae"/> -->
                <mesh filename="../meshes/dae/bp3_dae/BP3_LOWER.dae"/>
            </geometry>
            <material name="greyblack"/>
        </visual>
        <inertial>
            <mass value="0.5"/>
            <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0"/>
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
        </inertial>
        <collision>
            <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0"/>
            <geometry>
                <cylinder length="0.0245" radius="0.048"/>
            </geometry>
        </collision>
    </link>

<!---============================= Left Arm ======================-->
    <!--abad link-->
    <link name="abad_l">
        <visual>
            <origin rpy="0 0.0 0.0" xyz="-0.35 0 0.25"/>
            <geometry>
                <!-- <mesh filename="../meshes/dae/bp3_dae/BP3_ABAD_L.dae"/> -->
                <mesh filename="../meshes/human_dae/upper_l.dae"/>
            </geometry>
            <material name="greyblack"/>
        </visual>
        <inertial>
            <mass value="0.45"/>
            <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0"/>
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
        </inertial> 
        <collision>
            <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0"/>
            <geometry>
                <cylinder length="0.0215" radius="0.048"/>
            </geometry>
        </collision>
    </link>

    <!--abad to upper joint-->
    <joint name="abad_upper_l" type="continuous">
        <axis xyz="1 0 0"/>
        <origin rpy="0 0 0" xyz="0.05 -0.1 0.9"/>
        <parent link="abad_l"/>
        <child link="upper_l"/>
        <limit effort="200" lower="-3.14" upper="3.14" velocity="20"/>
        <dynamics damping="0.01" friction="0.2"/>
    </joint>

    <!--upper link-->
    <link name="upper_l">
        <visual>
            <origin rpy="0 0.0 0" xyz="-0.85 0.1 -0.65"/>
            <geometry>
                <!-- <mesh filename="../meshes/dae/bp3_dae/BP3_UPPER_R.dae"/> -->
                <mesh filename="../meshes/human_dae/lower_l.dae"/>
            </geometry>
            <material name="greyblack"/>
        </visual>
        <inertial>
            <mass value="0.5"/>
            <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0"/>
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
        </inertial>
        <collision>
            <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0"/>
            <geometry>
                <cylinder length="0.023" radius="0.048"/>
            </geometry>
        </collision>
    </link>

    <!--upper to lower joint-->
    <!-- <joint name="upper_lower_l" type="continuous">
        <axis xyz="0 1 0"/>
        <origin rpy="0 0 0" xyz="0.05 -0.05 -0.22"/>
        <parent link="upper_l"/>
        <child link="lower_l"/>
        <limit effort="200" lower="-3.14" upper="3.14" velocity="20"/>
        <dynamics damping="0.01" friction="0.2"/>
    </joint> -->

    <!--lower link-->
    <link name="lower_l">
        <visual>
            <origin rpy="0.0 0.0 3.14" xyz="0.02 -0.032 0"/>
            <geometry>
                <!-- <mesh filename="../meshes/dae/bp_lower.dae"/> -->
                <mesh filename="../meshes/dae/bp3_dae/BP3_LOWER.dae"/>
            </geometry>
            <material name="greyblack"/>
        </visual>
        <inertial>
            <mass value="0.5"/>
            <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0"/>
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
        </inertial>
        <collision>
            <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0"/>
            <geometry>
                <cylinder length="0.0245" radius="0.048"/>
            </geometry>
        </collision>
    </link>

</robot>