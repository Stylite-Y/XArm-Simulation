<?xml version="1.0" ?>
<robot name="BlackPanther_2d">

    <link name="world"/>

    <link name="link1"/>

<!--    <joint name="worldTolink1" type="fixed">-->

<!--        <parent link="world"/>-->

<!--        <child link="link1"/>-->

<!--        <origin xyz="0 0 0"/>-->

<!--    </joint>-->

    <link name="root_x">
        <inertia>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <mass value="0.01"/>
            <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
        </inertia>
    </link>

    <link name="root_z">
        <inertia>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <mass value="0.01"/>
            <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
        </inertia>
    </link>

    <link name="body">
        <!-- contain 4 abad motor's mass and inertia -->
        <inertia>
            <origin rpy="0 0 0" xyz="0 0 -0.003"/>
            <mass value="5.88"/>
            <inertia ixx="0.016269" ixy="0" ixz="0" iyy="0.1508" iyz="0" izz="0.060989"/>
        </inertia>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <box size="0.3 0.2 0.1"/>
            </geometry>
        </collision>
    </link>

    <joint name="link1Toroot_x" type="continuous">
        <parent link="link1"/>
        <child link="root_x"/>
        <origin xyz="0 0 0"/>
        <axis xyz="1 0 0"/>
    </joint>

    <joint name="root_xToroot_z" type="continuous">
        <parent link="root_x"/>
        <child link="root_z"/>
        <origin xyz="0 0 0"/>
        <axis xyz="0 0 1"/>
    </joint>

    <joint name="root_zTobody" type="continuous">
        <parent link="root_z"/>
        <child link="body"/>
        <origin xyz="0 0 0"/>
        <axis xyz="0 1 0"/>
    </joint>
</robot>
