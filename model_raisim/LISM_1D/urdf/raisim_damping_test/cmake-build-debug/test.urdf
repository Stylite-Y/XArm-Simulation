<robot name="2DrobotArm">

    <link name="world"/>



    <link name="link1"/>



    <joint name="worldTolink1" type="fixed">

        <parent link="world"/>

        <child link="link1"/>

        <origin xyz="0 0 0"/>

    </joint>



    <link name="link2">

        <inertial>

            <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>

            <mass value="0.2"/>

            <inertia ixx="0.0" ixy="0" ixz="0" iyy="0.0" iyz="0" izz="0.0"/>

        </inertial>

    </link>

